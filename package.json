{
  "name": "NewSolo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "adb:d": "adb devices",
    "adb:r": "adb kill-server && adb start-server && adb devices",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "android:link": "react-native link",
    "android:run": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && react-native run-android",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "test": "jest",
    "api:run": "make setup && docker-compose up",
    "api:load-mock-data": "make run-cmd CMD=\"load-fixtures\""
  },
  "dependencies": {
    "axios": "^0.17.1",
    "bluebird": "^3.5.1",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "react": "16.0.0",
    "react-native": "0.50.4",
    "react-native-device-info": "^0.13.0",
    "react-native-easy-grid": "^0.1.15",
    "react-native-elements": "^0.18.5",
    "react-native-fetch-blob": "^0.10.8",
    "react-native-fs": "^2.9.7",
    "react-native-image-resizer": "^1.0.0",
    "react-native-interactive-avatar": "^2.0.0",
    "react-native-linear-gradient": "^2.4.0",
    "react-native-opentok": "^2.0.1",
    "react-native-segmented-control-tab": "^3.2.1",
    "react-native-star-rating": "^1.0.8",
    "react-native-ui-kitten": "^2.0.1",
    "react-native-vector-icons": "^4.4.2",
    "react-navigation": "1.0.0-beta.21",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "remote-redux-devtools": "^0.5.12"
  },
  "devDependencies": {
    "babel-jest": "21.2.0",
    "babel-preset-react-native": "4.0.0",
    "jest": "21.2.1",
    "react-test-renderer": "16.0.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "rnpm": {
    "assets": [
      "src/Assets/Fonts"
    ]
  }
}
